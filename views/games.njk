{% extends "layout.njk" %}

{% block top%}{% endblock %}
{% block contenu%}

  {% if gamesAll %}
    {% for game in gamesAll%}
      <a href="/game/{{game._id}}">
        <div>
          <h3>{{game.name}}</h3>
          <img src="{{game.cover.url}}"/>
        </div>
      </a>
    {% endfor %}
  {% endif %}
{% endblock %}
