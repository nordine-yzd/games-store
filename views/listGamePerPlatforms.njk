{% extends "layout.njk" %}

{% block top%}{% endblock %}
{% block contenu%}

  {% if gamesAll %}
    {% for game in gamesAll%}
      <a href="/game/{{game._id}}">
        <div>
          <h3>{{game.platform.name}}</h3>
          <img src="{{game.platform.platform_logo_url}}"/>
        </div>
      </a>
    {% endfor %}
  {% endif %}
{% endblock %}

